{
  "expo": {
    "name": "TOO HOT",
    "slug": "its2hot-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "its2hotapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "privacy": "public",
    "primaryColor": "#3b82f6",
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cd9501a1-6d26-4451-ab0a-54631514d4fe"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.romp.its2hot",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to provide accurate temperature alerts for your area.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSApplePayUsageDescription": "This app uses Apple Pay to process t-shirt purchases securely.",
        "PKPaymentNetworks": ["visa", "mastercard", "amex", "discover"],
        "UIBackgroundModes": ["remote-notification"],
        "UIRequiresFullScreen": false,
        "UIStatusBarStyle": "default",
        "UIViewControllerBasedStatusBarAppearance": true,
        "LSApplicationQueriesSchemes": ["https"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.romp.its2hot",
            "CFBundleURLSchemes": ["its2hotapp"]
          }
        ]
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": ["merchant.com.romp.its2hot"]
      },
      "accessibility": {
        "isVoiceOverEnabled": true,
        "isSwitchControlEnabled": true,
        "isReduceMotionEnabled": true,
        "isReduceTransparencyEnabled": true,
        "isIncreaseContrastEnabled": true,
        "isBoldTextEnabled": true,
        "isLargerTextEnabled": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "package": "com.romp.its2hot",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#1a1a1a"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "its2hot.org"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "accessibility": {
        "isAccessibilityEnabled": true,
        "isTalkBackEnabled": true,
        "isSwitchAccessEnabled": true,
        "isHighContrastEnabled": true,
        "isLargeTextEnabled": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-location",
      "expo-updates",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cd9501a1-6d26-4451-ab0a-54631514d4fe"
      }
    }
  }
}
